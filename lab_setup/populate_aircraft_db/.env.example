NEO4J_URI=neo4j+s://xxxxxxxx.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-password-here

# LLM provider for entity extraction: "openai" (default) or "anthropic"
LLM_PROVIDER=openai

# OpenAI — required for enrich (embeddings always use OpenAI; LLM when LLM_PROVIDER=openai)
OPENAI_API_KEY=sk-your-key-here

# Anthropic — required for enrich when LLM_PROVIDER=anthropic
# Install with: uv sync --extra anthropic
# ANTHROPIC_API_KEY=sk-ant-your-key-here
# ANTHROPIC_EXTRACTION_MODEL=claude-sonnet-4-5-20250929
